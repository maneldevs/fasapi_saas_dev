{
    "files.exclude": {
        "**/__pycache__": true
    },
    "terminal.integrated.defaultProfile.linux": "bash",
    "black-formatter.args": [
        "--line-length=120"
    ],
    "flake8.args": [
        "--max-line-length=120"
    ],
    "editor.rulers": [120],
    "material-icon-theme.files.customClones": [
        {
            "name": "persistence",
            "base": "database",
            "color": "pink-400",
            "lightColor": "pink-600",
            "folderNames": [
                "persistence"
            ]
        },
        {
            "name": "controller",
            "base": "controller",
            "color": "pink-400",
            "lightColor": "pink-600",
            "folderNames": [
                "controller"
            ]
        },
        {
            "name": "application",
            "base": "api",
            "color": "pink-400",
            "lightColor": "pink-600",
            "folderNames": [
                "application"
            ]
        },
        {
            "name": "core",
            "base": "core",
            "color": "purple-400",
            "lightColor": "purple-600",
            "folderNames": [
                "core"
            ]
        },
        {
            "name": "venv-custom",
            "base": "environment",
            "color": "blue-600",
            "lightColor": "blue-400",
            "folderNames": [
                ".venv",
                "venv"
            ]
        },
        {
            "name": "src-custom",
            "base": "src",
            "color": "green-600",
            "lightColor": "green-400",
            "folderNames": [
                "src"
            ]
        },
        {
            "name": "resources-custom",
            "base": "resource",
            "color": "red-500",
            "lightColor": "pink-300",
            "folderNames": [
                "resources"
            ]
        },
        {
            "name": "config-custom",
            "base": "config",
            "color": "blue-600",
            "lightColor": "blue-400",
            "folderNames": [
                "nginx"
            ]
        }
    ],
}